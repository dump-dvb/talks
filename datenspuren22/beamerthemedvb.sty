\mode<presentation>

% Requirement
\RequirePackage{tikz}
\RequirePackage{graphicx}
\RequirePackage{svg}
\RequirePackage{xcolor}
\RequirePackage{pdfpages}
%\RequirePackage{comicsans}

% Settings
\useinnertheme{dvb}
\useoutertheme{dvb}
\usecolortheme{dvb}

\setbeamertemplate{navigation symbols}{}
\setbeamertemplate{blocks}[rounded][shadow=true]  
\setbeamertemplate{item projected}{bg=darkred}
\setbeamertemplate{block title}{fg=darkred}
\setbeamertemplate{local structure}{fg=darkred}

\mode<all>
